var resultado=0;
function sumaYresta () {

var numero1 = parseInt(prompt("Ingrese el primer sumando: "));
var numero2 = parseInt(prompt("Ingrese el segundo sumando: "));

resultado = numero1 + numero2; 
alert ("El resultado es: " + resultado);

return resultado;

}

sumaYresta();
console.log ('respuesta: '+ resultado);
document.write ('respuesta: <br>' +resultado);


var a= prompt ("Ingrese un nuemero:")
function calcularPrecioTotal (precio, porcentajeImpuestos) {
var gastoEnvios= 10;
var precioConImpuestos= (1 + porcentajeImpuestos/100) * precio;
var precioTotal = precioConImpuestos + gastoEnvios;
return precioTotal.toFixed(2);


}

var precioTotal = calcularPrecioTotal (23.34, 16);


var numero = prompt ('Introduce un numero');
var resultado= parImpar ();
